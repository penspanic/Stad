<UserControl x:Class="Stad.View.Wpf.UI.Components.Navigator"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Stad.View.Wpf.UI.Components"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="800">
	<UserControl.Resources>
		<ControlTemplate x:Key="roundbutton" TargetType="Button">
		</ControlTemplate>
	</UserControl.Resources>
	<Border BorderThickness="2">

		<Grid Background="Aqua">
			<StackPanel Orientation="Horizontal" x:Name="_stackPanel">
				<Button Template="{StaticResource roundbutton}" Width="100"></Button>
				<Button Template="{StaticResource roundbutton}"></Button>
			</StackPanel>
		</Grid>
	</Border>
</UserControl>
