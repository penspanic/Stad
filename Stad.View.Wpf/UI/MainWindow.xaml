<Window x:Class="Stad.View.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stad.View.Wpf"
        mc:Ignorable="d"
        Title="Stad" Height="326" Width="737" MinWidth="800" MinHeight="450">
	<Grid x:Name="_grid_Full">
		<Grid.RowDefinitions>
			<RowDefinition Height="30"></RowDefinition>
			<RowDefinition></RowDefinition>
		</Grid.RowDefinitions>
		<Menu Grid.Row="0" IsMainMenu="True">
			<MenuItem Header="_Assembly" Name="Assembly_Open">
				<MenuItem Header="O_pen" Click="Assembly_Open_Click"/>
			</MenuItem>
			<MenuItem Header="_Data">
				<MenuItem Header="O_pen" Click="Data_Open_Click"/>
			</MenuItem>
		</Menu>
		<TabControl Grid.Row="1" Width="Auto" x:Name="TabControl" IsSynchronizedWithCurrentItem="True" Height="Auto">
			<Grid>
				<TabItem Grid.Column="0" Header="TabItem"/>
				<TabItem Grid.Column="1" Header="TabItem"/>
				<TabItem Grid.Column="2" Header="TabItem"/>
				<TabItem Grid.Column="3" Header="TabItem"/>
			</Grid>
		</TabControl>
	</Grid>
</Window>
